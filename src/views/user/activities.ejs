<div class="center-body">
  <h1>Activities</h1>
  <table class="table">
    <tr>
      <th>Action Name</th>
      <th>Created At</th>
      <th>Target Title</th>
      <th>Target Type</th>
    </tr>
    <% viewData.forEach(element => { %>
    <tr>
      <td><%= element.action_name %></td>
      <td><%= element.created_at %></td>
      <td><%= element.target_title %></td>
      <td><%= element.target_type %></td>
    </tr>
    <% }) %>
  </table>
</div>